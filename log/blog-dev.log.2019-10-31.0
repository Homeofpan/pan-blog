		for (XNode child : context.getChildren()) {
			// environmentid
			String id = child.getStringAttribute("id");
			// isSpecifiedEnvironment(id)idequals environment,environment
			// ?~?
			if (isSpecifiedEnvironment(id)) {
				// transactionManager;mybatisJDBC  MANAGED,

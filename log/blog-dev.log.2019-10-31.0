      public void registerAlias(Class<?> type) {
        //,
        String alias = type.getSimpleName();
        //@Alias
        Alias aliasAnnotation = type.getAnnotation(Alias.class);
        //
        if (aliasAnnotation != null) {
          //value
          alias = aliasAnnotation.value();
        }
        //
        registerAlias(alias, type);

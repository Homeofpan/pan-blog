// properties,XNodeproperties
private void propertiesElement(XNode context) throws Exception {
	if (context != null) {
		// namevaluesetproperties
		Properties defaults = context.getChildrenAsProperties();
		// properties resource
		String resource = context.getStringAttribute("resource");
		// properties url, resourceurl
		String url = context.getStringAttribute("url");
		if (resource != null && url != null) {
			throw new BuilderException(
					"The properties element cannot specify both a URL and a resource based property file reference.  Please specify one or the other.");
		}
		// propertiesProperties,putProperties
		// Properties extends Hashtable<Object,Object>,PropertiesHashtable
		if (resource != null) {
			defaults.putAll(Resources.getResourceAsProperties(resource));
		} else if (url != null) {
			defaults.putAll(Resources.getUrlAsProperties(url));
		}
		// configurationProperties
		// configurationmybatis
		// configurationget/set java
		// ,getVariables(),(null)
		// variablesconfiguration:protected Properties variables = new Properties();
		Properties vars = configuration.getVariables();
		if (vars != null) {
			defaults.putAll(vars);
		}
		// propertisset 
		parser.setVariables(defaults);
		// configuration
		configuration.setVariables(defaults);
	}
}



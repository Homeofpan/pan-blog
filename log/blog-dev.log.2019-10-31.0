                InputStream inputStream = Resources.getResourceAsStream(resource);
                //mapperXMLMapperBuilder
                XMLMapperBuilder mapperParser = new XMLMapperBuilder(inputStream, configuration, resource,
                  configuration.getSqlFragments());

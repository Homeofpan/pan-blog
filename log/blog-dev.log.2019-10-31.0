            // Search XML mapper that is not in the module but in the classpath.
            try {
              //classpath
              inputStream = Resources.getResourceAsStream(type.getClassLoader(), xmlResource);
            } catch (IOException e2) {
              // ignore, resource is not required
            }
          }
           //,
